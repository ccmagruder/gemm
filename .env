NVIM_BUILD_CMD="[ -d build ] && rm -rf build || echo && mkdir build && cmake -S gemm -B build && cmake --build build && ctest --test-dir build --output-on-failure && source clang-format.sh && source clangd-tidy.sh && build/tests/bGemm"
NVIM_TEST_CMD="cmake -S gemm -B build && cmake --build build && ctest --test-dir build --output-on-failure && source clang-format.sh && source clangd-tidy"
NVIM_RUN_CMD="[ -d build-release ] && rm -rf build-release || echo && mkdir build-release && cmake -S gemm -B build-release -DCMAKE_BUILD_TYPE=Release && cmake --build build-release && ctest --test-dir build-release --output-on-failure && build-release/tests/bGemm --benchmark_out=bGemm.json --benchmark_out_format=json --benchmark_time_unit=ms"

