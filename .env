NVIM_BUILD_CMD="[ -d build ] && rm -rf build || echo && mkdir build && cmake -S gemm -B build && cmake --build build && ctest --test-dir build --output-on-failure && source clang-format.sh --dry-run --verbose && source clang-format.sh -i && source clangd-tidy.sh --tqdm && ./benchmark.sh build/tests"
NVIM_TEST_CMD="cmake -S gemm -B build && cmake --build build && ctest --test-dir build --output-on-failure && source clang-format.sh --dry-run --verbose && source clang-format.sh -i && source clangd-tidy.sh --tqdm"
NVIM_RUN_CMD="[ -d build-release ] && rm -rf build-release || echo && mkdir build-release && cmake -S gemm -B build-release -DCMAKE_BUILD_TYPE=Release && cmake --build build-release && ctest --test-dir build-release --output-on-failure && ./benchmark.sh build-release/tests"

